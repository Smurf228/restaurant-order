<head>
    <link rel="stylesheet" href="css/Log-In.css"/>
</head>
    <div class="registration-cssave">
        <form action="index.html">
            <h1 class="text-center">Restaurant</h1>
            <div class="form-group">
                <input class="form-control item" type="text" name="username" maxlength="20" minlength="4" pattern="^[a-zA-Z0-9_.-]*$" id="username" placeholder="Email" required>
            </div>
            <div class="form-group">
                <input class="form-control item" type="password" name="Password" minlength="6" id="password" placeholder="Password" required>
            </div>
            <div class="form-group" >
                <button class="btn" type="submit">Log In</button>
            </div>
        </form>
    </div>
  </body>